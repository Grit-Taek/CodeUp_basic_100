#include <iostream>

using namespace std;

int main()
{
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    int res = 1;
    while(1)
    {
        if( (res%a == 0) && (res%b == 0) && (res%c == 0)) {
            printf("%d", res);
            break;
        }
        else
            res++;
    }

    return 0;
}
